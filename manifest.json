{
  "manifest_version": 3,
  "name": "F.A.D - (Fails At DSA)",
  "version": "1.1",
  "description": "Plays 'fahhh' when a LeetCode submission fails.",
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["fahhh.mp3"],
      "matches": ["https://leetcode.com/*"]
    }
  ]
}